fun puts(s: NullTerminatedString) -> I32;

enum SomeEnum: I32 {
    First = 4,
    Second,
}

fun main() -> I32 {
    foo(SomeEnum.First);
    foo(SomeEnum.Second);
    return SomeEnum.Second;
}

fun foo(e: SomeEnum) {
    if e == SomeEnum.First {
        puts("first");
    }
    if e == SomeEnum.Second {
        puts("second");
    }
}